    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:textStyle="bold"
        android:textColorHint="#1F618D"
        android:gravity="left"
        android:hint="COVID 19 ALERT"
        android:textCursorDrawable="@color/black"
        android:fontFamily="@font/futura_heavy_font"
        app:fontFamily="@font/futura_heavy_font"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="50dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="80dp"
        android:padding="10dp"
        android:background="@drawable/docvoucher_field_background"
        android:weightSum="2"

        android:id="@+id/international_PETRAV">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColorHint="#1F618D"
            android:gravity="left"
            android:textStyle="bold"
            android:hint="International Guidelines"
            android:background="@color/white"
            android:textCursorDrawable="@color/black"
            android:fontFamily="@font/futura_heavy_font"
            app:fontFamily="@font/futura_heavy_font"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="25dp"
            android:layout_weight="1.2"/>

        <ImageView
            android:layout_width="45dp"
            android:layout_height="60dp"
            android:src="@mipmap/coorna"
            android:layout_gravity="center"
            android:padding="5dp"
            android:layout_weight=".8"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="100dp"
        android:padding="10dp"
        android:background="@drawable/docvoucher_field_background"
        android:weightSum="2"
        android:id="@+id/domestic_PETRAV">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColorHint="#1F618D"
            android:gravity="left"

            android:hint="Domestic Guidelines"
            android:background="@color/white"
            android:textCursorDrawable="@color/black"
            android:fontFamily="@font/futura_heavy_font"
            app:fontFamily="@font/futura_heavy_font"
            android:layout_marginLeft="5dp"
            android:textStyle="bold"
            android:layout_marginTop="25dp"
            android:layout_weight="1.2"/>

        <ImageView
            android:layout_width="45dp"
            android:layout_height="60dp"
            android:src="@mipmap/coorna"
            android:layout_gravity="center"
            android:padding="5dp"
            android:layout_weight=".8"/>

    </LinearLayout>




